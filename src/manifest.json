{
    "name": "Twitch-Live",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/image/icon_16.png",
            "32": "/image/icon_32.png",
            "48": "/image/icon_48.png",
            "128": "/image/icon_128.png"
        }
    },
    "icons": {
        "16": "/image/icon_16.png",
        "32": "/image/icon_32.png",
        "48": "/image/icon_48.png",
        "128": "/image/icon_128.png"
    }
}